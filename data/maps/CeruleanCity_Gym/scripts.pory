end
}

script CeruleanCity_Gym_EventScript_Imakuni {
	//trainerbattle_single(TRAINER_(placeholder), CeruleanCity_Gym_Text_ImakuniIntro, CeruleanCity_Gym_Text_ImakuniDefeat)
	msgbox(("In both cards and dance,\nsoul and rhythm are the secret.\pHey, that sounds pretty cool!\nMaybe I'll become a poet!"), MSGBOX_DEFAULT)
end
}

script CeruleanCity_Gym_EventScript_GrassTrainer {
	//trainerbattle_single(TRAINER_(placeholder), CeruleanCity_Gym_Text_GrassTrainerIntro, CeruleanCity_Gym_Text_GrassTrainerDefeat)
	msgbox(("That CARD is really powerful!\pPack it with HITMONCHAN and\nELECTABUZZ and you'll be set!\pAnd hey, come back to me any time\nyou want another one!\pSCYTHER actually completely fell\nout of the meta, so I don't need it."), MSGBOX_DEFAULT)
end
}

script CeruleanCity_Gym_EventScript_DragonTrainer {
	msgbox(format("Haha!\nI'm the DRAGON-type trainer!\p...Or at least, I would be if\nDRAGON cards existed in Gen 3."), MSGBOX_NPC)
	end
}

script CeruleanCity_Gym_EventScript_FairyTrainer {
	msgbox(format("FAIRY-type?\nWhat's a FAIRY-type?\pNope, there's never been a type\nlike that before!\lYou must be misremembering!"), MSGBOX_NPC)
	end
}

script CeruleanCity_Gym_EventScript_Dad {
	lock
	faceplayer
	textcolor(0)
	msgbox(("DAD: {PLAYER}!\nWhat are you doing out here?\pDidn't you know I'm actually\na star in the TCG?\lInvisible Wall is strong!\pWell, never mind that.\nHow about you take a rest?"), MSGBOX_DEFAULT)
	//call the healing script
	msgbox(("DAD: There!\nBest of luck to you!"), MSGBOX_DEFAULT)
	release
	end
}

