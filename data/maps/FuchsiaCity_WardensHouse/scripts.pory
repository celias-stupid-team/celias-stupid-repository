FushciaCity_WardensHouse_MapScripts::
	.byte 0

FushciaCity_WardensHouse_EventScript_Warden::
	lock
	faceplayer
	goto_if_set FLAG_GOT_HM04, FushciaCity_WardensHouse_EventScript_ExplainStrength
	goto_if_set FLAG_HIDE_SAFARI_ZONE_WEST_GOLD_TEETH, FushciaCity_WardensHouse_EventScript_GiveGoldTeeth
	msgbox FushciaCity_WardensHouse_Text_HifFuffHefifoo, MSGBOX_YESNO
	call_if_eq VAR_RESULT, YES, FushciaCity_WardensHouse_EventScript_WardenYes
	call_if_eq VAR_RESULT, NO, FushciaCity_WardensHouse_EventScript_WardenNo
	release
	end

FushciaCity_WardensHouse_EventScript_GiveGoldTeeth::
	textcolor NPC_TEXT_COLOR_NEUTRAL
	playfanfare MUS_OBTAIN_TMHM
	message FushciaCity_WardensHouse_Text_GaveGoldTeethToWarden
	waitmessage
	waitfanfare
	msgbox FushciaCity_WardensHouse_Text_WardenPoppedInHisTeeth
	call EventScript_RestorePrevTextColor
	checkplayergender
	call_if_eq VAR_RESULT, MALE, FushciaCity_WardensHouse_EventScript_WardenThanksMale
	call_if_eq VAR_RESULT, FEMALE, FushciaCity_WardensHouse_EventScript_WardenThanksFemale
	giveitem_msg FushciaCity_WardensHouse_Text_ReceivedHM04FromWarden, ITEM_HM04
	setflag FLAG_GOT_HM04
	removeitem ITEM_GOLD_TEETH
	release
	end

FushciaCity_WardensHouse_EventScript_WardenThanksMale::
	msgbox FushciaCity_WardensHouse_Text_ThanksSonGiveYouSomething
	return

FushciaCity_WardensHouse_EventScript_WardenThanksFemale::
	msgbox FushciaCity_WardensHouse_Text_ThanksLassieGiveYouSomething
	return

FushciaCity_WardensHouse_EventScript_WardenYes::
	msgbox FushciaCity_WardensHouse_Text_AhHowheeHoHoo
	return

FushciaCity_WardensHouse_EventScript_WardenNo::
	msgbox FushciaCity_WardensHouse_Text_HeOhayHeHaHoo
	return

FushciaCity_WardensHouse_EventScript_ExplainStrength::
	msgbox FushciaCity_WardensHouse_Text_ExplainStrength
	release
	end

FushciaCity_WardensHouse_EventScript_DisplaySign1::
	msgbox FushciaCity_WardensHouse_Text_MonPhotosFossilsOnDisplay, MSGBOX_SIGN
	end

FushciaCity_WardensHouse_EventScript_DisplaySign2::
